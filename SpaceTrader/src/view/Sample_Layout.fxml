            <Label fx:id="pMachinesLabel" onContextMenuRequested="#removeSellMachines" onMouseClicked="#sellMachines" text="Units of Machines" />
            <Label fx:id="pNarcoticsLabel" onContextMenuRequested="#removeSellNarcotics" onMouseClicked="#sellNarcotics" text="Units of Narcotics" />
