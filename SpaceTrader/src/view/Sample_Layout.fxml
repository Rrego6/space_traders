         </children>
      </VBox>
      <VBox fx:id="playerInventory" alignment="TOP_CENTER" layoutY="47.0" prefHeight="172.0" prefWidth="172.0">
         <children>
            <Label fx:id="pWaterLabel" onMouseClicked="#sellWater" text="Units of Water" />
            <Label fx:id="pFursLabel" onContextMenuRequested="#removeSellFurs" onMouseClicked="#sellFurs" text="Units of Furs" />
